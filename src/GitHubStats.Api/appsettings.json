{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "System.Net.Http.HttpClient": "Warning"
    }
  },
  "AllowedHosts": "*",
  "GitHub": {
    "PersonalAccessTokens": [],
    "FetchMultiPageStars": false,
    "GraphQLEndpoint": "https://api.github.com/graphql",
    "RestApiEndpoint": "https://api.github.com",
    "MaxRetries": 3,
    "TimeoutSeconds": 30
  },
  "Cache": {
    "RedisConnectionString": null,
    "InstanceName": "GitHubStats:",
    "StatsCardTtlSeconds": 86400,
    "TopLangsCardTtlSeconds": 518400,
    "PinCardTtlSeconds": 864000,
    "GistCardTtlSeconds": 172800,
    "ErrorTtlSeconds": 600
  },
  "AccessControl": {
    "Whitelist": null,
    "GistWhitelist": null,
    "Blacklist": [],
    "ExcludeRepositories": []
  }
}
